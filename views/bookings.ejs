<h2 class="text-center">My Dashboard - My Bookings</h2>
<div class="row">
    <div class="col col-lg-8 m-auto">
        <%- include ('partials/usernav') %>
    </div>
</div>
<% if(bookings) { %>
    <div class="row">
    <% bookings.forEach(booking => {%>
        <div class="col col-lg-8 m-auto">
            <div class="card card-body">
                <h5 class="text-center"><%= booking.eventId %> <span><a class="btn btn-warning"  onclick="window.location='/cancel/<%= booking._id %>';" style="cursor: pointer;" >Cancel Event</a> | 
                    <a class="btn btn-primary"  onclick="window.location='/view/<%= booking.eventId %>';" style="cursor: pointer;" >View Receipt</a></span></h5>

            </div>
        </div>

        <% }) %>
    </div>
<% } else {%>

<% } %>